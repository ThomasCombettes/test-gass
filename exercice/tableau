#include <stdio.h>
#include <stdlib.h>


void affiche(int *tableau, int tailletableau);

int main()

    {
        int tableau [4]

        affiche(tableau, 4)

        return 0;
        
    }

void affiche(int *tableau, int tailletableau)

    {
        int i;

        for (i = 0, i < 4, i++)

        printf("%d\n", tableau[i]);
    }